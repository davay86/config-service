node{

    stage('Checkout') {
        checkout scm
    }

    stage('Release to QA'){
        sh "kubectl config --kubeconfig=/etc/.kube/config use-context aws_QA"
        sh "kubectl replace --kubeconfig=/etc/.kube/config  -f configuration-api-parent/configuration-api/kubernetes/config-api-qa-deployment.yml --record"
    }

    stage('Verify QA'){
        //verify QA manual or smoke test.
    }
}